{
  "name": "@m7eio/pob-general-workflow",
  "version": "0.0.1",
  "description": "POB general workflow",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
