  },
  "author": "M7e.io <contact@m7e.io>",
  "license": "MIT",
  "dependencies": {
    "@m7eio/pob-core": "workspace:^",
    "ethers": "^5.6.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@types/node": "^18.0.6",
    "typescript": "^4.0.2"
  }
}